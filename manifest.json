{
  "manifest_version": 2,

  "name": "My GW",
  "description": "Remembers your preferred regional GW site and automatically redirects to it",
  "version": "0.1",

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["*://*.games-workshop.com/*"],
      "js": ["jquery-3.2.1.min.js", "contentscript.js"]
    }
  ],

  "permissions": ["storage", "webRequest", "webRequestBlocking", "*://www.games-workshop.com/"]
}
